<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="shortcut icon" href="../../assets/ico/favicon.ico">

     <title>{% block title %} LolSearch{% endblock %}</title>
        
		<a href="{{ path('lolsearch_home') }}"> <img class="logo" src="{{ asset('bundles/projetlolsearch/images/lols1.png') }}" /> </a>
     <link rel="stylesheet" href="//code.jquery.com/ui/1.10.4/themes/smoothness/jquery-ui.css">
    <script src="//code.jquery.com/jquery-1.9.1.js"></script>
    <script src="//code.jquery.com/ui/1.10.4/jquery-ui.js"></script>
    
    <!-- Bootstrap core CSS -->
    <link href="{{ asset('css/bootstrap.min.css')}}" rel="stylesheet">

    <!-- Custom styles for this template -->
    <link href="{{ asset('css/navbar.css')}}" rel="stylesheet">
    {% block stylesheets %}<link rel="stylesheet" href="{{ asset('bundles/projetlolsearch/css/menu.css') }}" />{% endblock %}

    <!-- Just for debugging purposes. Don't actually copy this line! -->
    <!--[if lt IE 9]><script src="../../assets/js/ie8-responsive-file-warning.js"></script><![endif]-->

    <!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
      <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
    <![endif]-->
  </head>

  <body>
    <div class="container">
      <!-- Static navbar -->
      <div class="navbar navbar-default" role="navigation">
        <div class="container-fluid">
          <div class="navbar-header">
            <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
            
          </div>
          <div class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
             	<li ><a href="{{ path('lolsearch_home') }}">Accueil</a></li>
				<li><a href="{{path('lolsearch_recherche')}}">Rechercher un Joueur</a></li>
        <li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown">Lieux <b class="caret"></b></a>
          <ul class="dropdown-menu">
            <li><a href="{{path('lolsearch_ajouterLieux')}}">Ajouter un lieu</a></li>
            <li><a href="{{path('lolsearch_rechercherLieux')}}"> Rechercher un lieu</a></li>
          </ul>
        </li>
				
				{% if app.user%}{%else%}<li><a href="{{path('lolsearch_inscription')}}">Inscription</a></li>{%endif%}
				<li class="dropdown">
          <a href="#" class="dropdown-toggle" data-toggle="dropdown">Evenement <b class="caret"></b></a>
          <ul class="dropdown-menu">
            <li><a href="{{path('lolsearch_ajouterEvent')}}">Ajouter un evenement</a></li>
            <li><a href="{{path('lolsearch_rechercherEvent')}}"> Rechercher un evenement</a></li>
            <li><a href="{{path('lolsearch_voirEvent')}}"> Liste des evenements</a></li>
          </ul>
        </li>
				<li><a href="#">FAQ</a></li>
				<li ><a href="{{ path('lolsearch_contact') }}">Nous Contacter</a></li>
				

            </ul>
            
           

            <ul class="nav navbar-nav navbar-right">
            	{% if app.user%}{%else%}<li class="dropdown">
			          <a href="#" class="dropdown-toggle" data-toggle="dropdown">Connexion <b class="caret"></b></a>
			          <ul class="dropdown-menu">
			            
                             <form action="{{ path('login_check') }}" method="post">
                                 <label>Login</label>
                                 <input id="username" type="text" name="_username" placeholder= "{{ 'Username'|trans }}" autofocus />
                                 <label>Password</label>
                                 <input id="password" type="password" name="_password"  placeholder = "{{ 'Password'|trans}}" />
                                
                                 <button type="submit">Sign in</button>
                        </form>
                        
			          </ul>
			        </li>{%endif%}
             	{% if app.user%}
             	<li><a href="{{ path('lolsearch_profil') }}">Mon Profil</a></li>
             	<li>{% block user_block %}<a class="btn btn-default btn-xs pull-right" href="{{ path('logout') }}">{{ 'Logout'|trans }}</a>{% endblock %}</li> {%endif%}
            </ul>
          </div><!--/.nav-collapse -->
        </div><!--/.container-fluid -->
      </div>

      <!-- Main component for a primary marketing message or call to action -->
      
      {% block body %}{% endblock %}
    </div> <!-- /container -->

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.0/jquery.min.js"></script>
    <script src="{{ asset('js/bootstrap.min.js')}}"></script>
  </body>
  <footer></footer>
</html>
